{
	"presets": [
		"react",
		"stage-3",
		[
			"env",
			{
				"targets": {
					"electron": "2.0"
				}
			}
		]
	],
	"plugins": [
		"transform-class-properties",
		[
			"transform-runtime",
			{
				"polyfill": false,
				"regenerator": true
			}
		],
		[
			"transform-inline-environment-variables",
			{
				"include": [
					"GA_TRACKING_ID"
				]
			}
		]
	]
}
