[ignore]
<PROJECT_ROOT>/packages/*/**/dist/.*
<PROJECT_ROOT>/packages/*/**/.cache/.*
<PROJECT_ROOT>/packages/*/**/release/.*
<PROJECT_ROOT>/packages/*/**/resources/.*
<PROJECT_ROOT>/.git/.*
<PROJECT_ROOT>/node_modules/config-chain/test/broken.json
<PROJECT_ROOT>/docs/.*
<PROJECT_ROOT>/packages/nimbus-weather-core/dist/.*
<PROJECT_ROOT>/packages/nimbus-weather-electron/.cache/.*
<PROJECT_ROOT>/packages/nimbus-weather-electron/dist/.*
<PROJECT_ROOT>/packages/nimbus-weather-webapp/dev/.*

[include]
<PROJECT_ROOT>/../../node_modules/.*

[libs]
<PROJECT_ROOT>/flow-typed/nimbus-weather-core_vx.x.x.js
../../flow-typed/nimbus-weather-core_vx.x.x.js

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
